{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HttpClient": {
    "BnaClient": {
      "ClientName": "httpClientBna",
      "Policies": {
        "RetryAttemps": 3
      }
    }
  },
  "BnaService": {
    "Endpoint": "https://bna.com.ar/Cotizador/HistoricoPrincipales?id=billetes&filtroDolar=1&filtroEuro=0",
    "ValidationHtml": "Dolar U.S.A",
    "NoQuotation": "There are no pending quotes for that date."
  },
  "SlackHook": {
    "Url": "[SECRET KEY]",
    "Text": "Doppler Currency Service can't get the USD currency of the Bank of Nacion Argentina today, Please check Html format"
  } 
}
